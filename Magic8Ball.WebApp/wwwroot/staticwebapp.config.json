{
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [ "/_framework/*", "/css/*", "/images/*" ]
    },
    "globalHeaders": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, DELETE, PUT, PATCH, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, api_key, Authorization"
    }
}